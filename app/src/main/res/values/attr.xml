<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="myViewTitleText" format="string" />
    <attr name="myViewTitleTextColor" format="color" />
    <attr name="myViewTitleTextSize" format="dimension" />

    <declare-styleable name="MyDesignView">
        <attr name="myViewTitleText" />
        <attr name="myViewTitleTextColor" />
        <attr name="myViewTitleTextSize" />
    </declare-styleable>

    <attr name="firstColor" format="color" />
    <attr name="secondColor" format="color" />
    <attr name="circleWidth" format="dimension" />
    <attr name="speed" format="integer" />

    <declare-styleable name="CircleProcessBar">
        <attr name="firstColor" />
        <attr name="secondColor" />
        <attr name="circleWidth" />
        <attr name="speed" />
    </declare-styleable>
</resources>
